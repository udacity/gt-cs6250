$("#navSubnavBorderTR").css("display","");
$("#navShopAllButton").addClass("navShopAllButtonSubnav");
}}};
self.getStartingNavUpdate=function(){var elemText=function(e){return e.textContent?e.textContent:e.innerText;
};
var startingNavUpdate={};
var navbar=$("#navbar");
var isBeacon=navbar.hasClass("nav-beacon");
startingNavUpdate.templated=true;
