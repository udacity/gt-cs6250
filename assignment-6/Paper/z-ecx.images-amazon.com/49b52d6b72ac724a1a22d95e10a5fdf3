return;
}if(status.redirectUrl){pageUrl=status.redirectUrl;
}if(reload){self.debug("pageReload: flag=",reload);
return;
}else{if(!pageUrl){self.warning("reload","INVALID",pageUrl,status);
return;
}}reload=true;
