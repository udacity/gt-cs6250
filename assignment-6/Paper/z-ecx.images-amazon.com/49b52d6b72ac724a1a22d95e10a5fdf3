}else{if(!isBeacon&&$("#navSubnavRowTR").is(":visible")){$("#navSubnavRowTR").css("display","none");
$("#navSubnavBorderTR").css("display","none");
$("#navShopAllButton").removeClass("navShopAllButtonSubnav");
}}};
